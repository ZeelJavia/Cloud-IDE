# Backend configuration
PORT=3001

# JWT Secret (change this in production)
JWT_SECRET=replace-with-a-long-random-secret

# Google OAuth2 Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Base URLs
BASE_URL=http://localhost:3001
FRONTEND_URL=http://localhost:5173

# AI provider API Key (set one of these)
A4F_API_KEY=ddc-a4f-a682b07c94c24f52bdee3776ba7ab30b
# or
OPENAI_API_KEY=ddc-a4f-a682b07c94c24f52bdee3776ba7ab30b
AI_MODEL=provider-3/gpt-5-mini

# Frontend CORS origins (comma-separated)
FRONTEND_ORIGINS=http://localhost:5173,http://localhost:3000

# MongoDB connection (local example or Atlas URI)
MONGODB_URI=mongodb://127.0.0.1:27017/cloud-ide

# Enable database-backed project/file storage (true|false)
USE_DB_PROJECTS=false

# Optional: customize which Docker images to pre-pull on startup (comma-separated)
DOCKER_IMAGES=node:20-alpine,python:3.11-slim,gcc:12,eclipse-temurin:17-jdk

# Docker warm container reuse
DOCKER_REUSE=true
CONTAINER_TTL_SEC=600

# Input and history limits
MAX_RUN_HISTORY=100
STDIN_MAX_BYTES=1048576
ARG_MAX_COUNT=64
ARG_MAX_LEN=1024
ENV_MAX_COUNT=64
ENV_VAL_MAX_LEN=2048
